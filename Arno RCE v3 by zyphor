local Players = game:GetService("Players")
local UIS = game:GetService("UserInputService")

local player = Players.LocalPlayer
local PlayerGui = player:WaitForChild("PlayerGui")

if PlayerGui:FindFirstChild("SimplexSS_UI") then
	PlayerGui.SimplexSS_UI:Destroy()
end

local ScreenGui = Instance.new("ScreenGui")
ScreenGui.Name = "ARNI_RCE_SERVERSIDE"
ScreenGui.ResetOnSpawn = false
ScreenGui.Parent = PlayerGui

local WHITE = Color3.fromRGB(255,255,255)
local LIGHT = Color3.fromRGB(245,245,245)
local PANEL = Color3.fromRGB(250,250,250)
local DARK = Color3.fromRGB(30,30,30)
local BORDER = Color3.fromRGB(140,140,140)
local BLUE = Color3.fromRGB(0,120,215)

local Main = Instance.new("Frame",ScreenGui)
Main.Size = UDim2.new(0,800,0,450)
Main.Position = UDim2.new(0.5,-400,0.5,-225)
Main.BackgroundColor3 = LIGHT
Main.BorderSizePixel = 1
Main.BorderColor3 = BORDER
Main.Active = true

local TitleBar = Instance.new("Frame",Main)
TitleBar.Size = UDim2.new(1,0,0,32)
TitleBar.BackgroundColor3 = Color3.fromRGB(235,235,235)
TitleBar.BorderSizePixel = 1
TitleBar.BorderColor3 = BORDER

local Title = Instance.new("TextLabel",TitleBar)
Title.Size = UDim2.new(1,-120,1,0)
Title.Position = UDim2.new(0,8,0,0)
Title.BackgroundTransparency = 1
Title.Text = "Arno Ss - Simple Backdoor Executer"
Title.TextColor3 = DARK
Title.Font = Enum.Font.Gotham
Title.TextSize = 14
Title.TextXAlignment = Enum.TextXAlignment.Left

local function CreateWindowButton(text,pos)
	local b = Instance.new("TextButton",TitleBar)
	b.Size = UDim2.new(0,40,1,0)
	b.Position = UDim2.new(1,-pos,0,0)
	b.Text = text
	b.Font = Enum.Font.Gotham
	b.TextSize = 14
	b.BackgroundColor3 = TitleBar.BackgroundColor3
	b.BorderSizePixel = 0
	b.TextColor3 = DARK
	b.MouseEnter:Connect(function()
		b.BackgroundColor3 = BLUE
		b.TextColor3 = WHITE
	end)
	b.MouseLeave:Connect(function()
		b.BackgroundColor3 = TitleBar.BackgroundColor3
		b.TextColor3 = DARK
	end)
	return b
end

local MinBtn = CreateWindowButton("—",120)
local MaxBtn = CreateWindowButton("□",80)
local CloseBtn = CreateWindowButton("X",40)

CloseBtn.MouseButton1Click:Connect(function()
	ScreenGui:Destroy()
end)

local dragging, dragInput, dragStart, startPos

TitleBar.InputBegan:Connect(function(input)
	if input.UserInputType == Enum.UserInputType.MouseButton1 then
		dragging = true
		dragStart = input.Position
		startPos = Main.Position
		input.Changed:Connect(function()
			if input.UserInputState == Enum.UserInputState.End then
				dragging = false
			end
		end)
	end
end)

TitleBar.InputChanged:Connect(function(input)
	if input.UserInputType == Enum.UserInputType.MouseMovement then
		dragInput = input
	end
end)

local dragging, dragInput, dragStart, startPos

local function update(input)
	local delta = input.Position - dragStart
	Main.Position = UDim2.new(
		startPos.X.Scale, 
		startPos.X.Offset + delta.X, 
		startPos.Y.Scale, 
		startPos.Y.Offset + delta.Y
	)
end

TitleBar.InputBegan:Connect(function(input)
	if input.UserInputType == Enum.UserInputType.MouseButton1 or input.UserInputType == Enum.UserInputType.Touch then
		dragging = true
		dragStart = input.Position
		startPos = Main.Position

		input.Changed:Connect(function()
			if input.UserInputState == Enum.UserInputState.End then
				dragging = false
			end
		end)
	end
end)

TitleBar.InputChanged:Connect(function(input)
	if input.UserInputType == Enum.UserInputType.MouseMovement or input.UserInputType == Enum.UserInputType.Touch then
		dragInput = input
	end
end)

UIS.InputChanged:Connect(function(input)
	if input == dragInput and dragging then
		update(input)
	end
end)


