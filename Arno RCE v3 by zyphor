local startX = (800 - totalWidth) / 2

local function CreateBottomButton(name,index)
	local b = Instance.new("TextButton",ButtonsBar)
	b.Size = UDim2.new(0,buttonWidth,0,28)
	b.Position = UDim2.new(0,startX + (index-1)*(buttonWidth+spacing),0,6)
	b.Text = name
	b.Font = Enum.Font.Gotham
	b.TextSize = 13
	b.BackgroundColor3 = WHITE
	b.BorderSizePixel = 1
	b.BorderColor3 = BORDER
	b.TextColor3 = DARK
	b.MouseEnter:Connect(function()
		b.BackgroundColor3 = BLUE
		b.TextColor3 = WHITE
	end)
	b.MouseLeave:Connect(function()
		b.BackgroundColor3 = WHITE
		b.TextColor3 = DARK
	end)
	return b
end

