local colorMapping = {
    ["local"] = "rgb(255, 0, 0)", 
    ["function"] = "rgb(85, 255, 85)",
    ["require"] = "rgb(110, 153, 202)",
    ["then"] = "rgb(85, 170, 255)",
    ["and"]="rgb(85, 170, 255)", ["break"]="rgb(85, 170, 255)", ["do"]="rgb(85, 170, 255)", 
    ["else"]="rgb(85, 170, 255)", ["elseif"]="rgb(85, 170, 255)", ["end"]="rgb(85, 170, 255)", 
    ["false"]="rgb(85, 170, 255)", ["for"]="rgb(85, 170, 255)", ["if"]="rgb(85, 170, 255)", 
    ["in"]="rgb(85, 170, 255)", ["nil"]="rgb(85, 170, 255)", ["not"]="rgb(85, 170, 255)", 
    ["or"]="rgb(85, 170, 255)", ["repeat"]="rgb(85, 170, 255)", ["return"]="rgb(85, 170, 255)", 
    ["true"]="rgb(85, 170, 255)", ["until"]="rgb(85, 170, 255)", ["while"]="rgb(85, 170, 255)", 
    ["game"]="rgb(85, 170, 255)", ["workspace"]="rgb(85, 170, 255)", ["script"]="rgb(85, 170, 255)"
}

local function highlightText(text)
    text = text:gsub("&", "&amp;"):gsub("<", "&lt;"):gsub(">", "&gt;")
    text = text:gsub("(%d+%.?%d*)", "<font color='rgb(215, 169, 75)'>%1</font>")
    for keyword, color in pairs(colorMapping) do
        text = text:gsub("%f[%a]" .. keyword .. "%f[%A]", "<font color='" .. color .. "'>" .. keyword .. "</font>")
    end
    return text
end

local function UpdateEditor()
    local content = TextBox.Text
    local lines = content:split("\n")
    local lineText = ""
    for i = 1, #lines do lineText = lineText .. i .. "\n" end
    Numbers.Text = lineText
    Highlight.Text = highlightText(content)
    
    local h = #lines * 18
    Scroll.CanvasSize = UDim2.new(0, 5000, 0, h + 50)
    Highlight.Size = UDim2.new(0, 5000, 0, h + 50)
    TextBox.Size = Highlight.Size
end

TextBox:GetPropertyChangedSignal("Text"):Connect(UpdateEditor)



